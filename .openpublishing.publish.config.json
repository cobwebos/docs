{
  "build_entry_point": "docs",
  "need_generate_pdf": false,
  "need_generate_intellisense": false,
  "enable_push_aggregation": true,
  "enable_pull_request_aggregation": true,
  "docsets_to_publish": [
    {
      "docset_name": "azure-documents",
      "build_source_folder": ".",
      "build_output_subfolder": "azure",
      "locale": "zh-cn",
      "version": 0,
      "open_to_public_contributors": true,
      "type_mapping": {
        "Conceptual": "Content",
        "ManagedReference": "Content",
        "RestApi": "Content"
      },
      "build_entry_point": "docs",
      "template_folder": "_themes"
    }
  ],
  "notification_subscribers": [
    "dengyun@microsoft.com",
    "MORDPS@microsoft.com"
  ],
  "branches_to_filter": [
    "master",
    "master-sync-work"
  ],
  "skip_source_output_uploading": false,
  "need_preview_pull_request": false,
  "git_repository_url_open_to_public_contributors": "https://github.com/Microsoft/azure-docs.zh-cn",
  "git_repository_branch_open_to_public_contributors": "live",
  "enable_incremental_build": true,
  "dependent_repositories": [
    {
      "path_to_root": "_themes",
      "url": "https://github.com/Microsoft/templates.docs.msft.zh-cn",
      "branch": "master",
      "branch_mapping": {}
    },
    {
       "path_to_root": "_repo.en-us",
       "url": "https://github.com/Microsoft/azure-docs-pr",
       "branch": "live",
       "branch_mapping": {}
    },
    {
      "path_to_root": "cli_scripts",
      "url": "https://github.com/Azure/azure-docs-cli-python-samples.git",
      "branch": "master"
    },
    {
      "path_to_root": "powershell_scripts",
      "url": "https://github.com/Azure/azure-docs-powershell-samples.git",
      "branch": "master"
    }
  ]
}
